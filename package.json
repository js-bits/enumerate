{
  "name": "@js-bits/enumerate",
  "version": "1.0.3",
  "description": "Easy to use, Symbol-based enum implementation",
  "keywords": [
    "javascript",
    "typescript",
    "symbol",
    "enum",
    "enumerate",
    "enumerable",
    "string",
    "number",
    "template",
    "literal",
    "primitive"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "test-cjs": "jest --verbose",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-watch": "yarn test --watch",
    "lint": "tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}'",
    "build": "rimraf ./dist && rollup ./index.js --format cjs --file dist/index.cjs --exports default",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/enumerate"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "dependencies": {
    "@js-bits/typedef-utils": "^1.0.0"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^1.0.3",
    "@js-bits/log-in-color": "^0.3.1",
    "@types/jest": "^29.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node",
        "testRegex": "(\\.|/)(test|spec)\\.[j]sx?$"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom",
        "testRegex": "(\\.|/)(test|spec)\\.[j]sx?$"
      }
    ]
  }
}
