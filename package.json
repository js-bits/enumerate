{
  "name": "@js-bits/enumerate",
  "version": "0.4.2",
  "description": "Easy to use, Symbol-based enum implementation",
  "keywords": [
    "javascript",
    "symbol",
    "enum",
    "enumerate",
    "enumerable",
    "string",
    "number",
    "template",
    "literal",
    "primitive"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch",
    "lint": "eslint '**/*.{js,jsx}'",
    "build-cjs": "rollup ./index.js --format cjs --file dist/cjs/index.js --exports default",
    "build-esm": "rollup ./index.js --format esm --file dist/esm/index.js",
    "build": "yarn build-cjs && yarn build-esm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/enumerate"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "devDependencies": {
    "@js-bits/formalinter": "^0.1.2",
    "@js-bits/log-in-color": "^0.0.2",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "rollup": "^2.55.1"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom"
      }
    ]
  }
}
